    width: auto;
    float: none;
    margin-bottom: 15px;
  }
  #related-list ul li {
    display: inline-block;
    padding-right: 10px;
  }
}
@media all and (max-width: 895px) {
  header #main-top-block #top_main_nav {
    margin-left: 15px;
  }
  header #main-top-block #inner-search-block {
    margin-right: 15px;
  }
  header #logo p {
    font-size: 3.3em;
  }
}
